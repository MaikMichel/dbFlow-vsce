{
	"name": "dbflow",
	"publisher": "MaikMichel",
	"icon": "images/icon.png",
	"license": "LICENSE",
	"repository": {
		"type": "git",
		"url": "https://github.com/MaikMichel/dbFlow-vsce"
	},
	"bugs": "https://github.com/MaikMichel/dbFlow-vsce/issues",
	"displayName": "dbFlow-vsce",
	"description": "An extension to let you compile PL/SQL, Javascript and CSS towards your APEX Instance",
	"version": "0.5.0",
	"engines": {
		"vscode": "^1.54.0"
	},
	"preview": false,
	"author": {
		"name": "Maik Michel"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:apply.env",
		"workspaceContains:xcl.yml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"taskDefinitions": [
			{
				"type": "dbFlow"
			}
		],
		"problemMatchers": [
			{
				"name": "dbflow-plsql",
				"owner": "external",
				"fileLocation": [
					"relative",
					"${workspaceFolder}"
				],
				"pattern": [
					{
						"regexp": "(WARNING|ERROR) (.*)",
						"severity": 1,
						"code": 2
					},
					{
						"regexp": "(.*?):(\\d+):(\\d+)",
						"file": 1,
						"line": 2,
						"column": 3
					},
					{
						"regexp": "(.*)",
						"message": 1,
						"loop": true
					}
				]
			}
		],
		"commands": [
			{
				"command": "dbFlow.compileFile",
				"title": "Compile current File",
				"category": "dbFlow",
				"enablement": "inDbFlowProject"
			},
			{
				"command": "dbFlow.exportAPEX",
				"title": "Export APEX Application",
				"category": "dbFlow",
				"enablement": "inDbFlowProject"
			},
			{
				"command": "dbFlow.exportREST",
				"title": "Export REST Module",
				"category": "dbFlow",
				"enablement": "inDbFlowProject"
			},
			{
				"command": "dbFlow.addREST",
				"title": "Add REST Module",
				"category": "dbFlow",
				"enablement": "inDbFlowProject"
			},
			{
				"command": "dbFlow.addAPP",
				"title": "Add APEX Application",
				"category": "dbFlow",
				"enablement": "inDbFlowProject"
			},
			{
				"command": "dbFlow.addStaticFolder",
				"title": "Add static folder for APP",
				"category": "dbFlow",
				"enablement": "inDbFlowProject"
			},
			{
				"command": "dbFlow.addReportFolder",
				"title": "Add report folder with prompted type",
				"category": "dbFlow",
				"enablement": "inDbFlowProject"
			},
			{
				"command": "dbFlow.executeTests",
				"title": "Execute utPLSQL tests",
				"category": "dbFlow",
				"enablement": "inDbFlowProject"
			}
		],
		"keybindings": [
			{
				"command": "dbFlow.compileFile",
				"key": "ctrl+alt+b",
				"mac": "ctrl+alt+b",
				"when": "inDbFlowProject"

			},
			{
				"command": "dbFlow.exportAPEX",
				"key": "ctrl+alt+e",
				"mac": "ctrl+alt+e",
				"when": "inDbFlowProject"
			},
			{
				"command": "dbFlow.exportREST",
				"key": "ctrl+alt+r",
				"mac": "ctrl+alt+r",
				"when": "inDbFlowProject"
			}
		],
		"configuration": [
			{
				"type": "object",
				"title": "dbFlow",
				"properties": {
					"dbFlow.cliToUseForCompilation": {
						"type": "string",
						"default": "SQL*Plus",
						"enum": [
							"SQL*Plus",
							"SQLcl"
						],
						"description": "Which CLI should be used to compile your source to specified database. Please make sure to set binaries to path",
						"scope": "window"
					},
					"dbFlow.showWarningMessages.AfterCompilation": {
						"type": "boolean",
						"default": false,
						"description": "If set to true additional output concerning warning messages is rendered to output",
						"scope": "window"
					},
					"dbFlow.showWarningMessages.AfterCompilationExcludingFollowingCodes": {
						"type": "array",
						"default": [
							"5018",
							"7203",
							"6009",
							"6010",
							"7204",
							"6023"
						],
						"description": "Warning Codes to exclude. These values will have an effect only when Show Warning Messages is set to true",
						"scope": "window"
					},
					"dbFlow.customTriggerRuns": {
						"type": "array",
						"items": {
							"type": "object",
							"title": "run config",
							"properties": {
								"triggeringExpression": {
									"type": "string",
									"description": "whenever a file is compile matching that regex"
								},
								"runFile": {
									"type": "string",
									"description": "SQL/PLSQL file to run. the triggering file will be the first parameter"
								},
								"runFileParameters": {
									"type": "array",
									"description": "Parameters to pass to SQL/PLSQL file"
								}
							}
						},
						"description": "Define regex to run files after you compile a file",
						"scope": "window"
					},
					"dbFlow.javaScriptModification.createAndUploadJavascriptMinifiedVersion": {
						"type": "boolean",
						"default": true,
						"description": "Create a minified version or your JavaScript file",
						"scope": "window"
					},
					"dbFlow.javaScriptModification.createAndUploadJavascriptSourceMap": {
						"type": "boolean",
						"default": true,
						"description": "Create a source map of your minified JavaScript file",
						"scope": "window"
					},
					"dbFlow.cssModification.createAndUploadCSSMinifiedVersion": {
						"type": "boolean",
						"default": true,
						"description": "Create a minified version or your CSS file",
						"scope": "window"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"esbuild": "^0.11.15",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"dotenv": "^8.2.0",
		"handlebars": "^4.7.7",
		"terser": "^5.6.1",
		"uglifycss": "0.0.29",
		"yaml": "^1.10.2"
	}
}
